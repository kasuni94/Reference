<Window x:Class="SimpleWebClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleWebClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        
            <TextBox x:Name="SID" HorizontalAlignment="Left" Margin="327,47,0,0" TextWrapping="Wrap" Text="Id" VerticalAlignment="Top" Width="120"/>
            <TextBox x:Name="SName" HorizontalAlignment="Left" Margin="327,79,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="120"/>
            <TextBox x:Name="SAge" HorizontalAlignment="Left" Margin="327,109,0,0" TextWrapping="Wrap" Text="Age" VerticalAlignment="Top" Width="120"/>
            <TextBox x:Name="SIndex" HorizontalAlignment="Left" Margin="327,151,0,0" TextWrapping="Wrap" Text="index?" VerticalAlignment="Top" Width="60"/>
            <Button x:Name="SearchButton" Content="Search" HorizontalAlignment="Left" Height="18" Margin="402,151,0,0" VerticalAlignment="Top" Width="45" Click="SearchButton_Click"/>
        <Button x:Name="AddButton" Content="ADD" HorizontalAlignment="Left" Margin="472,151,0,0" VerticalAlignment="Top" Click="AddButton_Click"/>


    </Grid>
</Window>
